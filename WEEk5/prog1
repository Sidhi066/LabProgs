#include <stdio.h>

int main() {
    int n, i, min, max, sum = 0;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    int arr[n];

    printf("Enter the elements: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
        if (i == 0) {
            min = max = arr[i];
        } else {
            if (arr[i] < min) min = arr[i];
            if (arr[i] > max) max = arr[i];
    
